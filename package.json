{
  "name": "tongxk",
  "version": "1.0.0",
  "description": "A comprehensive workspace with qiankun micro-frontend architecture",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky",
    "common": "pnpm --filter common",
    "app": "pnpm --filter web-app",
    "shared": "pnpm --filter shared",
    "shared:build": "pnpm --filter shared build",
    "shared:dev": "pnpm --filter shared dev",
    "vite-app": "pnpm --filter my-vite-app",
    "vite-app:dev": "pnpm --filter my-vite-app dev",
    "vite-app:build": "pnpm --filter my-vite-app build:prod",
    "vue2-app": "pnpm --filter my-vue2-app",
    "vue2-app:dev": "pnpm --filter my-vue2-app dev",
    "vue2-app:build": "pnpm --filter my-vue2-app build",
    "react-app": "pnpm --filter my-react-app",
    "react-app:dev": "pnpm --filter my-react-app dev",
    "react-app:build": "pnpm --filter my-react-app build",
    "blog": "pnpm --filter my-blog",
    "blog:dev": "pnpm --filter my-blog dev",
    "blog:build": "pnpm --filter my-blog build",
    "webp": "node bin/webp.mjs",
    "config:sync": "find apps -mindepth 1 -maxdepth 1 -type d -exec mkdir -p {}/.vscode/ \\; -exec cp .vscode/*.json {}/.vscode/ \\;",
    "rm-modules": "find . -type d -name 'node_modules' -exec rm -rf {} \\;",
    "build:all": "pnpm -r build",
    "dev:all": "pnpm -r dev",
    "dev:sub-apps": "pnpm --parallel --filter my-vite-app --filter my-react-app --filter my-vue2-app dev"
  },
  "keywords": [
    "micro-frontend",
    "qiankun",
    "vue",
    "react",
    "nuxt",
    "workspace"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "husky": "^9.1.7",
    "imagemin": "^9.0.0",
    "imagemin-webp": "^8.0.0",
    "lint-staged": "^15.4.3",
    "vue": "^3.2.37",
    "preset": "workspace:*",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "vue-router": "^4.5.0"
  }
}
